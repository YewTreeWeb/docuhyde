	"scripts": {
		"start": "gulp build",
		"build": "gulp build --prod",
		"lint": "markdownlint --config linters/.markdownlint.json README.md */README.md; eslint src/js/**/*.js; stylelint src/styles/**/*.scss --cache",
		"lint:fix": "eslint --fix src/js/**/*.js; stylelint src/styles/**/*.scss --fix; markdownlint **/*.md --fix;",
		"mlint": "markdownlint --config linters/.markdownlint.json **/*.md",
		"mlint:fix": "markdownlint --config linters/.markdownlint.json **/*.md --fix",
		"eslint": "eslint src/js/**/*.js",
		"eslint:fix": "eslint --fix src/js/**/*.js",
		"stylelint": "stylelint src/styles/**/*.scss --cache",
		"stylelint:fix": "stylelint src/styles/**/*.scss --fix",
		"p": "prettier --write 'src/**/*.js'",
		"p:w": "onchange 'src/js/**/*.js' -- prettier --write {{changed}}",
		"test": "jest",
		"webpack": "webpack --config=config/webpack.config.js"
	},
