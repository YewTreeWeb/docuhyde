{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "corejs": "3",
      "targets": {
        "browsers": [
          "last 5 versions",
          "> 1%",
          "ie >= 8"
        ]
      }
    }],
    "airbnb"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-transform-async-to-generator",
    [
      "@babel/transform-runtime",
      {
        "corejs": 3
      }
    ],
    ["@babel/plugin-syntax-module-attributes", {
      "version": "may-2020"
    }]
  ]
}
